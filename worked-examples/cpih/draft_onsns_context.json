{
    "@context": {
        "@language": "en",
        "dcterms": "http://purl.org/dc/terms/",
        "dcat": "http://www.w3.org/ns/dcat#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "onsns": "https://example.org/ons/ns#",
        "vcard": "http://www.w3.org/2006/vcard/ns#",
        "identifier": {
            "@id": "dcterms:identifier",
            "@language": null
        },
        "title": {
            "@id": "dcterms:title"
        },
        "summary": {
            "@id": "dcterms:abstract"
        },
        "description": {
            "@id": "dcterms:description",
            "@type": "https://www.iana.org/assignments/media-types/text/markdown"
        },
        "issued": {
            "@type": "xsd:dateTime",
            "@id": "dcterms:issued"
        },
        "nextRelease": {
            "@type": "xsd:dateTime",
            "@id": "onsns:nextRelease"
        },
        "publisher": {
            "@type": "@id",
            "@id": "dcterms:publisher",
            "@context": {
                "@base": "https://www.gov.uk/government/organisations/"
            }
        },
        "contact_point": {
            "@type": "vcard:Individual",
            "@id": "dcat:contactPoint",
            "@context": {
                "email": {
                    "@type": "@id",
                    "@id": "vcard:hasEmail",
                    "@context": {
                        "@base": "mailto:"
                    }
                },
                "name": {
                    "@id": "vcard:fn"
                },
                "telephone": {
                    "@type": "@id",
                    "@id": "vcard:hasTelephone",
                    "@context": {
                        "@base": "tel:"
                    }
                }
            }
        },
        "theme": {
            "@type": "@id",
            "@id": "dcat:theme",
            "@context": {
                "@base": "https://statistics.gov.uk/id/theme/"
            }
        },
        "frequency": {
            "@type": "@id",
            "@id": "dcterms:accuralPeriodicity",
            "@context": {
                "@base": "http://reference.data.gov.uk/def/intervals/"
            }
        },
        "keywords": {
            "@id": "dcat:keyword"
        }
    }
}